{
  "name": "code-agent-eval",
  "version": "0.0.0",
  "description": "TypeScript library for evaluating prompts against coding agents (Claude Code, Cursor, etc.) with multi-iteration testing and scoring",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/pffigueiredo/code-agent-eval#readme",
  "bugs": {
    "url": "https://github.com/pffigueiredo/code-agent-eval/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pffigueiredo/code-agent-eval.git"
  },
  "author": "Pedro Figueiredo",
  "keywords": [
    "code-agent",
    "coding-assistant",
    "claude-code",
    "cursor",
    "evaluation",
    "testing",
    "ai",
    "prompts",
    "agent-sdk",
    "eval",
    "llm",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run typecheck && tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "npm run build && bumpp",
    "release:alpha": "npm run build && bumpp --preid alpha --prerelease && npm publish"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "0.1.28",
    "execa": "9.6.0",
    "fs-extra": "11.2.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@types/node": "24.9.1",
    "bumpp": "10.3.1",
    "typescript": "5.9.3",
    "tsdown": "0.15.9",
    "vitest": "4.0.1"
  }
}
